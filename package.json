    "async",
    "promise",
    "timeout"
  ],
  "devDependencies": {
    "mocha": "^6.2.0",
    "rollup": "^1.21.4",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-terser": "^6.0.3"
